<h1>Policy</h1>

<h2>Customer</h2>
<p>name: <%= @params.quote.customer.name %></p>
<p>phone: <%= @params.quote.customer.email %></p>
<p>email: <%= @params.quote.customer.phone %></p>

<h2>Vehicle</h2>
<p>make: <%= @params.quote.vehicle.make %></p>
<p>model: <%= @params.quote.vehicle.year_model %></p>
<p>version: <%= @params.quote.vehicle.version %></p>

<%= button("Qualitas", to: "/quote/#{@params.quote.token}/qualitas", method: :post, class: "btn btn-success") %>
<%= button("Ana", to: "/quote/#{@params.quote.token}/ana", method: :post, class: "btn btn-success") %>
<%= button("Aba", to: "/quote/#{@params.quote.token}/aba", method: :post, class: "btn btn-success") %>
