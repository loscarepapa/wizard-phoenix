<h1>Address</h1>

<%= form_for @changeset, "/quotes/#{@params.token}", [method: "patch"], fn f -> %>
  <label>
    street: <%= text_input f, :street %>
  </label>
  <label>
    external_number: <%= text_input f, :external_number %>
  </label>
  <label>
    external_number: <%= text_input f, :country %>
  </label>
  <br>
  <%= submit "submit" %>
<% end %>

